<form data-ajax="true" data-ajax-mode="replace" data-ajax-method="POST" enctype="multipart/form-data" data-ajax-success="onSuccessUploadQuestions">
    <div id="upload-questions" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="info-header-modalLabel" aria-hidden="true">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #DB4B73">
                    <h4 class="modal-title text-white">Upload Questions</h4>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true"></button>
                </div>
                <div class="modal-body">
                    <div class="row mb-3">
                        <div class="col-md-3">
                            <label class="form-label required">Class:</label>
                            <input type="text" class="form-control" value="10" disabled>
                        </div>
                        <div class="col-md-3">
                            <label class="form-label required">Code:</label>
                            <input type="text" class="form-control" value="201" disabled>
                        </div>
                        <div class="col-md-3">
                            <label class="form-label required">Subject:</label>
                            <input type="text" class="form-control" value="English" disabled>
                        </div>
                        <div class="col-md-3">
                            <label class="form-label required">Paper Type:</label>
                            <input type="text" class="form-control" value="ePCP Final Paper" disabled>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-md-2">
                            <label class="form-label required">Select a File</label>
                        </div>
                        <div class="col-md-6">
                            <input type="file" class="form-control">
                        </div>
                        <div class="col-md-4 text-md-end">
                            <button type="submit" class="btn text-white mt-2" style="background-color: #DB4B73" value="Save">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
